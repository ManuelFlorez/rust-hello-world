fn main() {    
    
    let vector = vec![
        "monday",
        "tuesday",
        "wednesday",
        "thursday",
        "friday",
        "saturday",
        "sunday"
    ];

    let mut counter = 0;
    loop {
        println!("{}", vector[counter]);
        counter += 1;
        if counter == vector.capacity() {
            break;
        }
    }

    println!("--------------------------");

    counter = 0;
    while counter < vector.capacity() {
        println!("{}", vector[counter]);
        counter += 1;
    }

    println!("--------------------------");

    for number in 0..vector.capacity() - 1 {
        println!("{}", vector[]);
    }
}
